{
  "$schema": "https://raw.githubusercontent.com/sourcegraph/sourcegraph/main/client/shared/src/schema/extension.schema.json",
  "name": "css-intel",
  "icon": "data:image/png;base64,",
  "publisher": "vovakulikov",
  "displayName": "CSS Intel",
  "description": "Provides CSS rules MDN documentation, highlights and provides preview color and css variables inline search",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vovakulikov/sourcegraph-css-intel"
  },
  "categories": [
    "Linters",
    "Programming languages"
  ],
  "tags": [],
  "activationEvents": [
   "*"
  ],
  "targets": {
    "default": {
      "includeNodeModules": true
    },
    "main": false
  },
  "wip": true,
  "contributes": {
    "actions": [],
    "menus": {
      "editor/title": [],
      "commandPalette": []
    },
    "configuration": {}
  },
  "main": "dist/css-intel.js",
  "scripts": {
    "eslint": "eslint 'src/**/*.ts'",
    "typecheck": "tsc -p tsconfig.json",
    "build": "parcel build --out-file dist/css-intel.js src/css-intel.ts --no-source-maps --no-minify",
    "symlink-package": "mkdirp dist && lnfs ./package.json ./dist/package.json",
    "serve": "yarn run symlink-package && parcel serve --no-hmr --no-source-maps --out-file dist/css-intel.js src/css-intel.ts",
    "watch:typecheck": "tsc -p tsconfig.json -w",
    "watch:build": "tsc -p tsconfig.dist.json -w",
    "sourcegraph:prepublish": "yarn run typecheck && yarn run build",
    "test": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' mocha"
  },
  "browserslist": [
    "last 1 Chrome versions",
    "last 1 Firefox versions",
    "last 1 Edge versions",
    "last 1 Safari versions"
  ],
  "mocha": {
    "recursive": true,
    "extensions": "ts",
    "timeout": 200,
    "spec": "src/**/*.test.ts",
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  },
  "devDependencies": {
    "@sourcegraph/eslint-config": "^0.22.0",
    "@sourcegraph/tsconfig": "^4.0.1",
    "@types/lodash": "^4.14.175",
    "@types/mocha": "^8.2.0",
    "@types/mock-require": "^2.0.0",
    "@types/shelljs": "^0.8.8",
    "eslint": "^7.20.0",
    "lnfs-cli": "^2.1.0",
    "mkdirp": "^1.0.4",
    "mocha": "^8.3.0",
    "mock-require": "^3.0.3",
    "parcel-bundler": "^1.12.4",
    "shelljs": "^0.8.4",
    "source-map-support": "^0.5.19",
    "sourcegraph": "^25.3.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "postcss": "^8.3.8",
    "postcss-scss": "^4.0.1",
    "tagged-template-noop": "^2.1.1"
  }
}
